<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Carnovum</title>

    <link href="https://fonts.googleapis.com/css?family=Crimson+Text:400,400i,600|Montserrat:200,300,400" rel="stylesheet">

    <link rel="stylesheet" href="~/css/bootstrap/bootstrap.css">
    <link rel="stylesheet" href="~/fonts/ionicons/css/ionicons.min.css">

    <link rel="stylesheet" href="~/fonts/fontawesome/css/font-awesome.min.css">

    
    <link rel="stylesheet" href="~/css/slick.css">
    <link rel="stylesheet" href="~/css/slick-theme.css">

    <link rel="stylesheet" href="~/css/helpers.css">
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/landing-2.css">
    @RenderSection("Csses", required: false)
</head>
<body data-spy="scroll" data-target="#pb-navbar" data-offset="200">
    <nav class="navbar navbar-expand-lg pb_navbar pb_" id="pb-navbar">
        <div class="container">
            <a class="navbar-brand" asp-page="/Publico/Index">
                <img src="~/images/logo.png" alt="pyv" width="200" />
            </a>
            <button class="navbar-toggler ml-auto" type="button" data-toggle="collapse" data-target="#probootstrap-navbar" aria-controls="probootstrap-navbar" aria-expanded="false" aria-label="Toggle navigation">
                <span><i class="ion-navicon"></i></span>
            </button>
            <div class="collapse navbar-collapse" id="probootstrap-navbar">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item"><a class="nav-link" asp-page="/Publico/Index">INICIO</a></li>
                    <li class="nav-item"><a class="nav-link" href="/Publico/Index#section-servicios">SERVICIOS</a></li>
                    <li class="nav-item"><a class="nav-link" href="/Publico/Index#section-reviews">REVIEWS</a></li>
                    <li class="nav-item"><a class="nav-link" asp-page="/Publico/QuienesSomos">QUIÉNES SOMOS</a></li>
                    <li class="nav-item"><a class="nav-link" asp-page="/Publico/Contacto"><span class="pb_rounded-4">CONTÁCTANOS</span></a></li>
                </ul>
            </div>
        </div>
    </nav>

    @RenderBody()


 
    <!-- END section -->
    <footer class="pb_footer bg-black" role="contentinfo">
        <div class="container">
            <div class="row">
                <div class="col-md-5">
                    <img src="~/images/logo.png" alt="pyv" width="200" />
                </div>
                <div class="col-md-7">
                    <div class="row">
                        <div class="col-md-4">
                            Sitio
                        </div>
                        <div class="col-md-4">
                           Comunicaciones
                        </div>
                        <div class="col-md-4">
                            Contacto
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- loader -->
    <div id="pb_loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#1d82ff" /></svg></div>




    <script src="~/js/jquery.min.js"></script>

    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/slick.min.js"></script>
    <script src="~/js/jquery.mb.YTPlayer.min.js"></script>

    <script src="~/js/jquery.waypoints.min.js"></script>
    <script src="~/js/jquery.easing.1.3.js"></script>

    <script src="~/js/main.js"></script>


    @RenderSection("Scripts", required: false)
</body>
</html>
